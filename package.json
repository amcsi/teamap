{
  "name": "Teamap",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "analyze": "node tools/scripts/analyze",
    "android": "react-native run-android",
    "build": "tsc && node tools/scripts/build",
    "clean": "node tools/scripts/clean",
    "deploy": "node tools/scripts/deploy",
    "development": "node tools/development",
    "flow": "node tools/scripts/flow",
    "flow:defs": "flow-typed install -f 0.34 --overwrite",
    "flow:report": "flow-coverage-report -i 'src/**/*.js' -t html -t json -t text",
    "flow:remove": "node tools/scripts/removeTypes.js && rimraf tools/scripts/removeTypes.js",
    "lint": "eslint src",
    "start": "node build/server",
    "start-native": "node node_modules/react-native/local-cli/cli.js start --root build/src",
    "test": "jest"
  },
  "dependencies": {
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-native": "0.36.0"
  },
  "jest": {
    "preset": "jest-react-native"
  },
  "devDependencies": {
    "app-root-path": "2.0.1",
    "assets-webpack-plugin": "3.5.0",
    "babel-cli": "6.18.0",
    "babel-core": "6.18.2",
    "babel-eslint": "7.1.0",
    "babel-jest": "16.0.0",
    "babel-loader": "6.2.7",
    "babel-plugin-transform-class-properties": "6.18.0",
    "babel-plugin-transform-es2015-destructuring": "6.18.0",
    "babel-plugin-transform-object-rest-spread": "6.16.0",
    "babel-preset-latest": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-react-native": "1.9.0",
    "chokidar": "1.6.1",
    "code-split-component": "^2.0.0-alpha.2",
    "colors": "1.1.2",
    "compression": "1.6.2",
    "css-loader": "0.25.0",
    "dotenv": "2.0.0",
    "eslint": "3.9.1",
    "eslint-config-airbnb": "12.0.0",
    "eslint-plugin-flowtype": "2.25.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.5.0",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "0.9.0",
    "flow-bin": "0.34.0",
    "flow-coverage-report": "0.1.0",
    "flow-remove-types": "1.0.4",
    "flow-typed": "2.0.0",
    "glob": "7.1.1",
    "happypack": "2.2.1",
    "helmet": "3.0.0",
    "hpp": "0.2.1",
    "jest": "16.0.2",
    "jest-react-native": "16.0.0",
    "json-loader": "0.5.4",
    "md5": "2.2.1",
    "node-notifier": "4.6.1",
    "node-uuid": "1.4.7",
    "normalize.css": "5.0.0",
    "path": "0.12.7",
    "react-helmet": "3.1.0",
    "react-hot-loader": "3.0.0-beta.6",
    "react-router": "4.0.0-alpha.5",
    "react-test-renderer": "15.3.2",
    "regenerator-runtime": "0.9.5",
    "rimraf": "2.5.4",
    "serialize-javascript": "1.3.0",
    "source-map-support": "0.4.6",
    "style-loader": "0.13.1",
    "sw-precache-webpack-plugin": "0.5.1",
